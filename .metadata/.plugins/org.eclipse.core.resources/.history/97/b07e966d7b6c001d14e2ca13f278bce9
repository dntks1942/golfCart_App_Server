package CallByReference;

public class Test {
	public static void main(String[] args) {
		String input = new String("Testing");
		StringPass pass = new StringPass(input);
		
		pass.modify(input);
	}
}
class StringPass{
	String s;
	StringPass(String input){
		s = new String(input);
	}
	
	
	void modify(String input) {
		s = input;
		System.out.println(s);
	}
}
